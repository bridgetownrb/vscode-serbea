{
    "name": "serbea",
    "displayName": "Serbea",
    "publisher": "bridgetown",
    "description": "",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "serbea",
            "aliases": ["Serbea (Ruby)", "serbea"],
            "extensions": [".serb"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "serbea-markdown",
            "aliases": ["Serbea Markdown (Ruby)"],
            "extensions": [".md"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "serbea",
            "scopeName": "text.html.serb",
            "path": "./syntaxes/serbea.tmLanguage.json"
        },
        {
            "language": "serbea-markdown",
            "scopeName": "text.html.markdown.serb",
            "path": "./syntaxes/serbea-markdown.tmLanguage.json"
        }]
    }
}